ALGORITHM algorithm_name
VAR
    STRING str ;
    Integer nbW :=0
    INTEGER nbV :=0;


BEGIN
    i=0;
  while (str[i]!=".")  do {
       if(str[i]==" "){
          nbw:=nbw+1;
       }
       if(str[i]=="a" || str[i] =="A" || str[i] =="E"||str[i]=="e" || str[i] =="U"||str[i]=="u" || str[i] =="I"|| str[i] =="i" ||str[i]=="o" ||str[i]=="O" ||str[i]=="y" ||str[i]=="Y"){
        nbV:=nbV+1;
       } 
     
     i:=i+1;
    }

    write(nbW);
    write(nbV);

   END